{
  "name": "amazon_tests",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "clean_test_data": "rm -rf cypress/screenshots cypress/videos cypress/results cypress/reports mochawesome-report cucumber_report",
    "e2eheadless": "cypress run",
    "e2e:chrome": "cypress run --browser chrome",
    "merge_reports": "mochawesome-merge --reportDir mochawesome-report > mochawesome-report/output.json",
    "generate_mochawesome_report": "marge mochawesome-report/output.json",
    "e2e_mochawesome": "cypress run; npm run merge_reports; npm run generate_mochawesome_report",
    "cucumber_html_report": "node report.js",
    "docker:run:chrome:electron": "docker-compose up",
    "docker:build:chrome": "docker build -t amazon_tests_chrome .",
    "docker:build:electron": "docker build -t amazon_tests_electron .",
    "docker:run:chrome": "docker run --shm-size 512M amazon_tests_chrome --browser chrome",
    "docker:run:electron": "docker run --shm-size 512M amazon_tests_electron --headless",

    "eslint:fix": "./node_modules/.bin/eslint --fix --ignore-path .gitignore ."
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "cucumber": "^7.0.0-rc.0",
    "cucumber-html-reporter": "^5.3.0",
    "cypress": "^6.5.0",
    "cypress-cucumber-preprocessor": "^4.0.1",
    "cypress-multi-reporters": "^1.4.0",
    "eslint": "^7.20.0",
    "eslint-config-strongloop": "^2.1.0",
    "mocha": "^8.3.0",
    "mocha-junit-reporter": "^2.0.0",
    "mochawesome": "^6.2.2",
    "mochawesome-merge": "^4.2.0",
    "mochawesome-report-generator": "^5.2.0"
  },
  "cypress-cucumber-preprocessor": {
    "step_definitions": "cypress/integration/step_definitions/**",
    "cucumberJson": {
      "generate": true,
      "outputFolder": "cucumber_report",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/andge86/amazon_tests.git"
  }
}
